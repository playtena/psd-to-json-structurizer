<?xml version="1.0" encoding="UTF-8"?>

<ExtensionManifest Version="4.0" ExtensionBundleId="com.plexonic.pstj" ExtensionBundleVersion="0.0.3"
		ExtensionBundleName="PsdToJson" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.plexonic.pstj" Version="0.1" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="PHXS" Version="[14.0,14.9]" />
			<Host Name="PHSP" Version="[14.0,14.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="en_US" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="4.0" />

		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.plexonic.pstj">
			<DispatchInfo >
				<Resources>
				<MainPath>./index.html</MainPath>
				<ScriptPath>./PsdToJson.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>P2J</Menu>
					<Geometry>
						<Size>
							<Height>150</Height>
							<Width>150</Width>
						</Size>
					</Geometry>
					<Icons>
						<Icon Type="Normal">./icons/icon-active.png</Icon>
						<Icon Type="RollOver">./icons/icon-over.png</Icon>
						<Icon Type="Disabled">./icons/icon-disable.png</Icon>
						<Icon Type="DarkNormal">./icons/icon-dark-active.png</Icon>
						<Icon Type="DarkRollOver">./icons/icon-dark-over.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>